end


struc foobar { foo bar baz }

asm		dynFunNew @_dump_struc
		push "dump_struc"
	envAdd

	jmp @_skip_0

_dump_struc:
        getmem -1+(foobar.bar)
        #getmem 10
	+$tab
        getmem -1+(foobar.foo)
        #getmem 10
	+$tab
        getmem -1+(foobar.baz)
        #getmem 11
	+$ret
        print 6
	ret 0

_skip_0:
	nop
end

